{
  "root": true,
  "extends": "@react-native-community",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "comma-dangle": "off",
    "block-spacing": "warn",
    "eol-last": "off",
    "prettier/prettier": 0,
    "no-mixed-spaces-and-tabs": "off"
  }
}
