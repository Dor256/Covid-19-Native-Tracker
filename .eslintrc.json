{
  "root": true,
  "extends": "@react-native-community",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "class-property"],
  "rules": {
    "comma-dangle": "off",
    "space-before-blocks": "warn",
    "eol-last": "off",
    "prettier/prettier": 0,
    "no-mixed-spaces-and-tabs": "off",
    "semi": "warn",
    "object-curly-spacing": ["warn", "always"]
  }
}
